# {{ ansible_managed }}
---
apiVersion: metallb.io/v1beta1
kind: IPAddressPool
metadata:
  name: {{ loadbalancer_pool_name }}
  namespace: metallb-system
spec:
  addresses:
    - {{ loadbalancer_pool_network | ipv4("1") | ipv4("address") }}-{{ loadbalancer_pool_network | ipv4("255") | ipv4("address") }}
...
---
apiVersion: metallb.io/v1beta1
kind: L2Advertisement
metadata:
  name: empty
  namespace: metallb-system
...
